{
  "manifest_version": 3,
  "name": "Smart Tabs",
  "description": "Smart Tabs: grouped browser tabs on the new tab page.",
  "version": "1.0.0",
  "permissions": ["tabs"],
  "host_permissions": ["<all_urls>"],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
} 